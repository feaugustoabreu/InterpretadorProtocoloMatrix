<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
    <Version>1.0.0.5</Version>
    <Description>Interpretador .NET para ler e escrever arquivos .RCB e .ENV utilizados no protocolo Matrix pelo MatrixConnect</Description>
    <Authors>Fernando de Abreu</Authors>
    <Company />
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Copyright>2020</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIconUrl>http://matrixsaude.com/wp-content/uploads/2018/09/Photo_Connect_600Ha.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/feaugustoabreu/InterpretadorProtocoloMatrix</PackageProjectUrl>
    <RepositoryUrl>https://github.com/feaugustoabreu/InterpretadorProtocoloMatrix</RepositoryUrl>
    <PackageTags>matrixconnect matrixsaude tm laboratorial protocolo matrix</PackageTags>
    <NeutralLanguage>pt-BR</NeutralLanguage>
    <PackageReleaseNotes>Adicionado a data no formato "yyyyMMddTHHmmss" no nome do arquivo .RCB a ser criado</PackageReleaseNotes>
    <AssemblyVersion>1.0.0.5</AssemblyVersion>
    <FileVersion>1.0.0.5</FileVersion>
    <UserSecretsId>5944a8f6-ce8e-49ed-bb4c-d5a4814e3607</UserSecretsId>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="Properties\" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="Microsoft.Build.Framework" />
    <Reference Include="System.ComponentModel.DataAnnotations" />
  </ItemGroup>
</Project>